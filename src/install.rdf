<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

<Description about="urn:mozilla:install-manifest">
    <em:type>2</em:type>
    <em:id>fb2reader@clear.com.ua</em:id>
    <em:name>FB2 Reader</em:name>
    <em:version>0.28.1</em:version>
    <em:creator>Timothy Babych</em:creator>
    <em:description>Read FB2 e-Books with Firefox</em:description>
    <!-- To serve Firefox 3 with proper 32x32 icon -->
    <em:iconURL>chrome://fb2reader_icon/skin/icon.png</em:iconURL>
    <em:optionsURL>chrome://fb2reader/content/options.xul</em:optionsURL>
    <em:homepageURL>https://github.com/tymofij/fb2reader</em:homepageURL>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>37.0</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Pale Moon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>25.0</em:minVersion>
        <em:maxVersion>25.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- SeaMonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.9</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Firefox Mobile is not supported. https://bugzilla.mozilla.org/show_bug.cgi?id=621406 -->

    <em:localized>
      <Description>
        <em:locale>uk</em:locale>
        <em:name>FB2 читалка</em:name>
        <em:creator>Тимофій Бабич</em:creator>
        <em:description>Читайте електронні книжки FB2 просто з Firefox’а.</em:description>
      </Description>
    </em:localized>

    <em:localized>
      <Description>
        <em:locale>ru</em:locale>
        <em:name>FB2 читалка</em:name>
        <em:creator>Тимофей Бабич</em:creator>
        <em:description>Читайте электронные книги FB2 прямо из Firefox’а.</em:description>
      </Description>
    </em:localized>

  </Description>
</RDF>
